            "cwd": "${workspaceFolder}/run",
            "vmArgs": "-Dforge.logging.console.level=debug -Dforge.logging.markers=SCAN,REGISTRIES,REGISTRYDUMP -XX:HeapDumpPath=MojangTricksIntelDriversForPerformance_javaw.exe_minecraft.exe.heapdump \"-Dos.name=Windows 10\" -Dos.version=10.0",
            "env": {
                "FORGE_GROUP": "net.minecraftforge",
                "FORGE_VERSION": "25.0.76",
                "MCP_MAPPINGS": "snapshot_20190311-1.13.2",
                "MCP_VERSION": "20190213.203750",
                "MC_VERSION": "1.13.2",
                "MOD_CLASSES":"thespotlightmod%%${workspaceFolder}/bin/main;thespotlightmod%%${workspaceFolder}/bin/main",
                "assetDirectory":"C:/Users/robin/.gradle/caches/forge_gradle/assets",
                "assetIndex": "1.13.1",
